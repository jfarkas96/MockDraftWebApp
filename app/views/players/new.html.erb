<h1>New Player</h1>
<%= form_for :player, url: players_path do |f| %>
    <p>
        <%= f.label :name %><br>
        <%= f.text_field :name %>
    </p>
    <p>
        <%= f.label :college %><br>
        <%= f.text_field :college %>
    </p>
    <p>
        <%= f.label :position %><br>
        <%= f.text_field :position %>
    </p>
    <p>
        <%= f.label :grade %><br>
        <%= f.text_field :grade %>
    </p>
    <p>
        <%= f.label :height %><br>
        <%= f.text_field :height %>
    </p>
    <p>
        <%= f.label :weight %><br>
        <%= f.text_field :weight %>
    </p>
    <p>
        <%= f.label :picture_link %><br>
        <%= f.text_field :picture_link %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>